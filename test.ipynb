 "# 转换日期格式\n",
 "song_data['Date'] = pd.to_datetime(song_data['Date'])\n",
 "\n",
 "# 按照歌曲和日期排序\n",
 "song_data_sorted = song_data.sort_values(by=['Title', 'Artists', 'Date'])\n",
 "\n",
 "# 计算相邻日期之间的差异\n",
 "song_data_sorted['date_diff'] = song_data_sorted.groupby(['Title', 'Artists'])['Date'].diff().dt.days\n",
 "\n",
 "# 如果日期差异大于1天或者是组的开始（即NaN），则开始新的计数\n",
 "song_data_sorted['new_group'] = (song_data_sorted['date_diff'] != 1).cumsum()\n",
 "\n",
 "# 计算每组内的连续天数\n",
 "song_data_sorted['consecutive_days'] = song_data_sorted.groupby(['Title', 'Artists', 'new_group']).cumcount() + 1\n",
 "\n",
 "# 显示部分数据以验证结果\n",
 "song_data_sorted[['Title', 'Artists', 'Date', 'date_diff', 'new_group', 'consecutive_days']].head()\n",
 "# 找到每首歌曲的最大连续上榜天数\n",
 "max_consecutive_days = song_data_sorted.groupby(['id'])['consecutive_days'].max().reset_index()\n",
 "\n",
 "# 重命名列以更清晰地表示其含义\n",
 "max_consecutive_days.rename(columns={'consecutive_days': 'max_consecutive_days'}, inplace=True)\n",
 "\n",
 "# 显示部分结果\n",
 "max_consecutive_days.head()"
]
